<div>

</div>

<h1>Friends</h1>
  <% @friendships.each do |friendship| %>
    <ul>
      <% if friendship.asker == current_user %>
        <li>Username: <%= friendship.receiver.username %></li>
      <% else %>
        <li>Username: <%= link_to friendship.asker.username, profile_path(friendship.asker.id) %></li>
      <% end %>
    </ul>
  <% end %>
<%= render "shared/bottom_navbar" %>
